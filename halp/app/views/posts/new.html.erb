<h1>New Post</h1>

<%= form_for :post, url: posts_index_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :photo_url %><br>
    <%= f.text_area :photo_url %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :resume %><br>
    <%= f.text_area :resume %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
